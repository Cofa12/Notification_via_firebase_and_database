{
    "name": "cofa/notification_via_firebase_and_database",
    "description": "This is a package for sending notificatinon via firebase and database with good code structure",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cofa\\NotificationViaFirebaseAndDatabase\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mahmoud Gamal",
            "email": "112869567+Cofa12@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "kreait/firebase-php": "^7.0|^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/config": "^10.0|^11.0|^12.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0"
    },

    "conflict": {
        "tymon/jwt-auth": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0|^12.0",
        "vimeo/psalm": "^5.0|^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cofa\\NotificationViaFirebaseAndDatabase\\FirebaseNotificationServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "psalm": "psalm --no-cache"
    }
}